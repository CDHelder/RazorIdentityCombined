@page
@model Razor_Identity.Pages.Parts.DetailsModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Details for @Html.DisplayFor(model => model.Part.Name)</h1>

<div>
    <h4>Part</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Part.Name)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Part.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Part.PartType)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Part.PartType)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Part.Price)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Part.Price)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Part.Id">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
